<Addin id          = "sBook"
       author      = "Rasmus Pedersen"
       copyright   = "MIT X11"
       url         = "http://www.sundown.dk/"
       description = "Facebook integration for the Sorento Engine"	   
       version     = "1.0">
	<Runtime>
		<Import assembly="sBook.dll"/>
	</Runtime>
	
	<Dependencies>
		<Addin id="Sorento" version="1.0"/>
		<Addin id="sConsole" version="2.0"/>
	</Dependencies>
	
	<Extension path = "/SorentoLib/Init">
		<Type type="sBook.Addin.Init"/>
	</Extension>	
					
	<Extension path = "/SorentoLib/Resolver/Ajaxes">
		<Type type="sBook.Addin.Ajax"/>
	</Extension>				
</Addin>